    "@types/graphql": "^14.5.0",
    "bcrypt": "^5.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "graphql-scalars": "^1.23.0",
    "mercurius": "^14.1.0",
    "mongodb": "^6.8.0",
    "nexus": "^1.3.0",
    "njwt": "^2.0.1",
    "pino": "^9.3.1",
    "rotating-file-stream": "^3.2.3",
